<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/save_file_alert_dialog"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:descendantFocusability="blocksDescendants"
    android:orientation="horizontal"
    android:paddingEnd="@dimen/alert_padding_right"
    android:paddingLeft="@dimen/alert_padding_left"
    android:paddingRight="@dimen/alert_padding_right"
    android:paddingStart="@dimen/alert_padding_left">

<EditText android:id="@+id/filename_edittext"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:hint="FILENAME"
    android:inputType="text"/>


</FrameLayout>